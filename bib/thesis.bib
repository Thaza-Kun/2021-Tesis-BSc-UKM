@article{Filho2016,
title = "Extended uncertainty from first principles",
journal = "Physics Letters B",
volume = "755",
pages = "367 - 370",
year = "2016",
issn = "0370-2693",
doi = "https://doi.org/10.1016/j.physletb.2016.02.035",
url = "http://www.sciencedirect.com/science/article/pii/S0370269316001313",
author = "Raimundo N. {Costa Filho} and João P.M. Braga and Jorge H.S. Lira and José S. Andrade",
abstract = "A translation operator acting in a space with a diagonal metric is introduced to describe the motion of a particle in a quantum system. We show that the momentum operator and, as a consequence, the uncertainty relation now depend on the metric. It is also shown that, for any metric expanded up to second order, this formalism naturally leads to an extended uncertainty principle (EUP) with a minimum momentum dispersion. The Ehrenfest theorem is modified to include an additional term related to a tidal force arriving from the space curvature introduced by the metric. For one-dimensional systems, we show how to map a harmonic potential to an effective potential in Euclidean space using different metrics."
}

